{
  "name": "what-genre-next",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start:build": "next build",
    "start:run": "NODE_ENV=production node server.js"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "http": "^0.0.1-security",
    "nanoid": "^3.1.22",
    "next": "10.2.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "url": "^0.11.0"
  },
  "devDependencies": {
    "@youngcapital/eslint-config-yc-react": "^2.0.0",
    "eslint": "^7.26.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.23.2",
    "prettier": "^2.2.1"
  },
  "eslintConfig": {
    "extends": "@youngcapital/eslint-config-yc-react",
    "settings": {
      "import/resolver": {
        "node": {
          "paths": [
            "."
          ]
        }
      }
